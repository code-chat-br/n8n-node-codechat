  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/CodeChatCredentialsApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/CodeChat/CodeChat.node.js"
    ]
  },
